<?xml version="1.0"?>
<launch>
	<param name="/use_sim_time" value="true" />
	
	<node name="rviz" pkg="rviz" type="rviz" args="-d $(find exp_devel)/rviz/empty_husky.rviz" output="screen"/>
	
	<node name="laser_hot_fix" pkg="exp_devel" type="laser_hot_fix"  output="screen">
		<param name="max_range" value="9.0" />
	</node>
	 
	<include file="$(find husky_navigation)/launch/move_base.launch" />
	
	<include file="$(find husky_navigation)/launch/gmapping.launch" />	

	<include file="$(find husky_gazebo)/launch/husky_empty_world.launch" >
		   <arg name="world_name" value="$(find exp_devel)/worlds/factory1.world"/>
		   	<arg name="x_ini" value="-5.0"/>
  			<arg name="y_ini" value="-22.0"/>
  			<arg name="z_ini" value="0.0"/>
	</include>
	

</launch>